#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL,"Russian");
    int a,b,c;
    cout<<"a=";
    cin>>a;
    cout<<"b=";
    cin>>b;
    c=a*b;
    cout<<a<<"*"<<b<<"=";
    cin>>c;
    if(c==a*b){
        cout<<"Решено верно!";
    }else{
        cout<<"Решено не верно!" <<endl;
        cout<<"Верное решение= "<<a*b;
    }

    return 0;
}
