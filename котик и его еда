#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL,"Russian");
    int n; //проверяет,что ест 8 котик
    cout<<"Выберите одного из десяти котиков =";
    cin>>n;
    switch(n){
    case 1:{cout<<"котик будет есть гурмет с курицей"; //то,что есть первый котик
    break;
    }
    case 2:{cout<<"котик будет есть гурмет с ягненком";
    break;
    }
    case 3:{cout<<"котик будет есть гурмет с говядиной";
   break;
    }
    case 4:{cout<<"котик будет есть гурмет с лососем";
    break;
    }
    case 5:{cout<<"котик будет есть гурмет с форелью";
    break;
    }
    case 6:{cout<<"котик будет есть гурмет с уткой";
    break;
    }
    case 7:{cout<<"котик будет есть гурмет с индейкой";
    break;
    }
    case 8:{cout<<"котик будет есть гурмет с кроликом по-французски";
    break;
    }
    case 9:{cout<<"котик будет есть гурмет с тунцом";
   break;
    }
    case 10:{cout<<"котик будет есть гурмет с курицей и печенью";
   break;
    }
    }
    return 0;
}
